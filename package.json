{
  "name": "twine-formatter-gd",
  "version": "0.1.0",
  "description": "A Twine Story Format for nostalgia",
  "scripts": {
    "build-step1": "node scripts/build.mjs --combineonly",
    "build-step1-es5": "node scripts/build.mjs --combineonly --es5",
    "build-step1-release": "node scripts/build.mjs --combineonly --release",
    "build-step1-release-es5": "node scripts/build.mjs --combineonly --release --es5",
    "build-sample": "node scripts/build.mjs --sample",
    "build-sample-es5": "node scripts/build.mjs --sample --es5",
    "build-sample-release": "node scripts/build.mjs --sample --release",
    "build-sample-release-es5": "node scripts/build.mjs --sample --release --es5",
    "clean": "node scripts/build.mjs --cleanall",
    "build": "node scripts/build.mjs",
    "build-es5": "node scripts/build.mjs --es5",
    "release": "node scripts/build.mjs --release",
    "release-es5": "node scripts/build.mjs --release --es5",
    "dev": "node scripts/dev-server.mjs",
    "dev-es5": "node scripts/dev-server.mjs --es5"
  },
  "author": "Raj Chaudhuri",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.25.0",
    "@minify-html/node": "^0.15.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "alive-server": "^1.3.0",
    "chokidar": "^3.6.0",
    "rollup": "^4.19.1"
  }
}
